{
  "name": "solana-example",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "migration": "docker compose down && docker compose up -d && rm -rf db/migrations/* && npx tsc && npx squid-typeorm-migration generate",

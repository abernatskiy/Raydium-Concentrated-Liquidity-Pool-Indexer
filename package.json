    "@solana/spl-token": "^0.4.12",
    "@solana/web3.js": "1",
    "@subsquid/batch-processor": "^0.0.0",
    "@subsquid/borsh": "^0.1.0",
    "@subsquid/solana-objects": "^0.0.1",
    "@subsquid/solana-stream": "^0.0.1",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "@types/bn.js": "^5.1.6",
    "add": "^2.0.6",
    "bn.js": "^5.2.1",
